#include<stdio.h>
void main()
{
    int a,b,c;
    printf("enter age of ram as a\n");
    printf("enter age of shyam as b\n");
    printf("enter age of ajay as c\n");
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c);
    if (a<b&&a<c)
    {
        printf("ram is youngest");
    }
    else if (b<a&&b<c)
    {
         printf("shyam is youngest");
    }
    else
    {
        printf("ajay is youngest");
    }
}
