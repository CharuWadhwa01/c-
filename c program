{
    printf("hello world \n");
    int i ,age;
    for(i=0;i<10;i++)
    {
    printf("%d \n enter your age\n" ,i);
    scanf("%d \n" ,&age);
    

    if(age>10)
    {
        continue;
    }
    printf("i am a good girl");
    printf("dont write code");
