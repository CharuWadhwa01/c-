#include<stdio.h>
int main(int argc, char const *argv[])
{
    int aadhar[10];
    int *ptr=&aadhar[0];
    for (int i = 0; i < 10; i++)
    {
        printf("%d enter index is:" ,i);
        scanf("%d" ,&aadhar[i]);

    }
    for (int i = 0; i <10; i++)
    {
        printf(" %d index =%d\n" ,i,aadhar[i]);

    }
    
    return 0;
    
}
