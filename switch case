#include <stdio.h>

int main()
{
    int age,marks;
    printf("enter your age");
    scanf("%d" ,&age);

    printf("enter your marks");
    scanf("%d" ,&marks);

    switch (age)
    {
    case 3:
    printf("age is 3");

    switch (marks)
    {
    case 45:
    printf("your marks are 45");
        break;
        default:("your marks RE NOT 45");
        
        break;
    }
    break;
    case 13:
    printf("your age is 13");
    break;

    case 16:
    printf("your age is 16");
    break;

    
    default:printf("your afe is not 3,13,16");
        break;
    }
    return 0;
}
