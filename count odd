#include<stdio.h>
int countodd(int arr[],int n);

int main(int argc, char const *argv[])
{
    int arr[100],n,countodd=0;
    printf("enter the size of an array\n");
    scanf("%d",&n);
    printf("enter the elements of aaray\n ");
    for (int i = 0; i < n; i++)
    {
        scanf("%d" ,&arr[i]);

    }
    for (int i = 0; i < n; i++)
    {
        if (arr[i]%2==1)
        {
            countodd++;
        }
        printf("count number count=%d\n",countodd);
        
    }
return 0;
        
}
